{
  "name": "the-source",
  "version": "0.0.1",
  "description": "Source language toolchain and standard library",
  "typings": "es6/index.d.ts",
  "main": "es5/index.js",
  "jsnext:main": "es6/index.js",
  "scripts": {
    "clean": "rimraf .awcache dist es6 es5",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "prepublish": "typings install && npm run lint && npm run compile",
    "compile": "NODE_ENV=production tsc -p . --declaration && babel es6 --out-dir es5 --ignore *.spec.js",
    "dist": "NODE_ENV=production node ./scripts/bundle",
    "test": "ava",
    "watch": "NODE_ENV=development concurrently 'tsc -p .  --watch' 'ava --watch --verbose'"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "eslint": "^3.0.1",
    "espree": "^3.1.6",
    "immutable": "^3.8.1",
    "rxjs": "^5.0.0-beta.10"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "awesome-typescript-loader": "^1.1.1", 
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.9.0",
    "concurrently": "^2.2.0",
    "debug": "^2.2.0",
    "json-loader": "^0.5.4",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.4",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "webpack": "^1.13.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evansb/the-source.git"
  },
  "keywords": [
    "compiler",
    "education"
  ],
  "ava": {
    "files": [
      "es6/**/*.spec.js"
    ],
    "source": [
      "es6/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "require": [
      "babel-register"
    ],
    "concurrency": 5,
    "babel": "inherit"
  },
  "author": "Evan Sebastian <evanlhoini@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evansb/source-toolchain/issues"
  },
  "homepage": "https://github.com/evansb/source-toolchain#readme"
}
